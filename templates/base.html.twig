    <nav class="navbar navbar-expand-lg navbar-light bg-info">
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link text-white" href="{{path('index')}}">Accueil</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-white" href="{{path('prod')}}">Produit</a>
                </li>
                
                {% if not app.user %}
                <li class="nav-item ">
                    <a class="nav-link text-white" href="{{path('security_registrationUser')}}">S'inscrire</a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link text-white" href="{{path('login')}}">Connexion</a>
                </li>
                {% endif %}
                {% if app.user %}
                <li class="nav-item">
                    <a class="nav-link text-white" href="{{ path('logout') }}">Deconnexion</a> 
                </li>
                <li class="nav-item">
                    <a class="nav-link text-white" href="{{ path('super_admin') }}">SuperAdmin</a> 
                </li>
                {% endif %}

            </ul>
        </div>
    
